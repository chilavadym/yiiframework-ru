{
    "name": "samdark/yiiframework-ru",
    "description": "yiiframework.ru",
    "keywords": ["yii", "yiiframework.ru"],
    "homepage": "http://www.yiiframework.ru/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/samdark/yiiframework-ru/issues?state=open",
        "forum": "http://yiiframework.ru/forum/",
        "source": "https://github.com/samdark/yiiframework-ru/"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
